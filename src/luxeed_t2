update
wait 500000
set ff 00 00 11
update
wait 500000
set ff 00 00 12
update
wait 500000
set ff 00 00 13
update

set 00 ff 00 10
update
wait 500000
set 00 ff 00 11
update
wait 500000
set 00 ff 00 12
update
wait 500000
set 00 ff 00 13
update
wait 500000

set 00 00 ff 10
update
wait 500000
set 00 00 ff 11
update
wait 500000
set 00 00 ff 12
update
wait 500000
set 00 00 ff 13
update
wait 500000

set 00 00 00 10
update
wait 500000
set 00 00 00 11
update
wait 500000
set 00 00 00 12
